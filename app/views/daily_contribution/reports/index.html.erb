<p id="notice"><%= notice %></p>

<h1>Listing Daily Contribution Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_contribution_reports.each do |daily_contribution_report| %>
      <tr>
        <td><%= link_to 'Show', daily_contribution_report %></td>
        <td><%= link_to 'Edit', edit_daily_contribution_report_path(daily_contribution_report) %></td>
        <td><%= link_to 'Destroy', daily_contribution_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_daily_contribution_report_path %>
