<p id="notice"><%= notice %></p>

<h1>Listing Reports Daily Contributions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports_daily_contributions.each do |reports_daily_contribution| %>
      <tr>
        <td><%= link_to 'Show', reports_daily_contribution %></td>
        <td><%= link_to 'Edit', edit_reports_daily_contribution_path(reports_daily_contribution) %></td>
        <td><%= link_to 'Destroy', reports_daily_contribution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

